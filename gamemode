#define GAMETEXT_COLOR 0

#define HOLDING(%0) \
	((newkeys & (%0)) == (%0))

#define RELEASED(%0) \
	(((newkeys & (%0)) != (%0)) && ((oldkeys & (%0)) == (%0)))

#define PRESSED(%0) \
        (((newkeys & (%0)) == (%0)) && ((oldkeys & (%0)) != (%0)))
#define IsPlayerNotInVehicle(%0) (!IsPlayerInAnyVehicle(%0))

new
	afk[ MAX_PLAYERS char ]
;

new VehicleModel[MAX_PLAYERS];
new CFCTimer;

forward CheckForChange(playerid);
forward ShowGameTextForPlayer(playerid);

