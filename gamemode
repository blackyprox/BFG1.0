public OnGameModeExit()
{
	return 1;
}

public OnPlayerRequestClass(playerid, classid)
{
	SetPlayerCameraPos(playerid, 2666.7036,2797.8311,17.6896);
    SetPlayerCameraLookAt(playerid, 2671.4568,2795.6125,17.6896);
    SetPlayerPos(playerid, 2671.4568,2795.6125,17.6896);
    SetPlayerFacingAngle(playerid, 48.8905);
	return 1;
}

public OnPlayerConnect(playerid)
{
    new pName[MAX_PLAYER_NAME], string[39 + MAX_PLAYER_NAME];
    GetPlayerName(playerid, pName, sizeof(pName));
    format(string, sizeof(string), "%s [ID:%d] has joined the server. [Connected]", pName, playerid);
    SendClientMessageToAll(0xAFAFAFAA, string);
    SendClientMessage(playerid,0x33CCFFAA,"Welcome to the server,use /help to start your journey.");
    return 1;
}

public OnPlayerDisconnect(playerid, reason)
{
    new pName[MAX_PLAYER_NAME], string[39 + MAX_PLAYER_NAME];
    GetPlayerName(playerid, pName, sizeof(pName));
    switch(reason)
    {
        case 0: format(string, sizeof(string), "%s [ID:%d] has left the server. [Lost Connection]", pName, playerid);
        case 1: format(string, sizeof(string), "%s [ID:%d] has left the server. [Leaving]", pName, playerid);
        case 2: format(string, sizeof(string), "%s [ID:%d] has left the server. [Kicked/Banned]", pName, playerid);
    }
    SendClientMessageToAll(0xAFAFAFAA, string);
    return 1;
}

public OnPlayerSpawn(playerid)
{
	return 1;
}

public OnPlayerDeath(playerid, killerid, reason)
{
	return 1;
}

public OnVehicleSpawn(vehicleid)
{
	return 1;
}

public OnVehicleDeath(vehicleid, killerid)
{
	return 1;
}

public OnPlayerText(playerid, text[])
{
    SetPlayerChatBubble(playerid, text, 0xFF0000FF, 100.0, 10000);
	return 1;
}

COMMAND:teles(playerid, params[])
{
        new Float:x,Float:y,Float:z;
        PlayerPlaySound(playerid,1133,x,y,z);
        new sz_msg [ 900 ];
        strcat( sz_msg, "Drift: /drift1-53\r\n");
        strcat( sz_msg, "Deathmatch: /smalldm\r\n");
        strcat( sz_msg, "Derby: /derby1-7\r\n");
        strcat( sz_msg, "Race: /bigrally\r\n");
        strcat( sz_msg, "Other: /bj1-32\r\n");
        ShowPlayerDialog(playerid, 1337, DIALOG_STYLE_MSGBOX, "Server Teleports", sz_msg, "Close","");
        return 1;
}

COMMAND:help(playerid, params[])
{
        new Float:x,Float:y,Float:z;
        PlayerPlaySound(playerid,1133,x,y,z);
        new sz_msg [ 900 ];
        strcat( sz_msg, "Use /v to open up a dialog to spawn a vehicle.\r\n");
        strcat( sz_msg, "You can repair your car by pressing 2!\r\n");
        strcat( sz_msg, "You can use /f to flip your car. \r\n");
        strcat( sz_msg, "Type /rules to see the rules.\r\n");
        strcat( sz_msg, "Type /cmds to see the commands.\r\n");
        strcat( sz_msg, "Type /teles to see the teleports.\r\n");
        strcat( sz_msg, "Use /admins to see if there is any admin online!\r\n");
        ShowPlayerDialog(playerid, 1337, DIALOG_STYLE_MSGBOX, "Server Help", sz_msg, "Close","");
        return 1;
}

COMMAND:cmds(playerid, params[])
{
        new Float:x,Float:y,Float:z;
        PlayerPlaySound(playerid,1133,x,y,z);
        new sz_msg [ 900 ];
        strcat( sz_msg, "/teles /help /rules /f /pm /kill /afk /back /v\r\n");
        strcat( sz_msg, "/admins /goto /cmenu\r\n");
        ShowPlayerDialog(playerid, 1337, DIALOG_STYLE_MSGBOX, "Server Commands", sz_msg, "Close","");
        return 1;
}

COMMAND:rules(playerid, params[])
{
        new Float:x,Float:y,Float:z;
        PlayerPlaySound(playerid,1133,x,y,z);
        new sz_msg [ 900 ];
        strcat( sz_msg, "1: No car jaking.\r\n");
        strcat( sz_msg, "2. No cursing over players.\r\n");
        strcat( sz_msg, "3. Do NOT spam or else you will be kicked!\r\n");
        strcat( sz_msg, "4. Do NOT advertise other servers or else you will be banned!\r\n");
        strcat( sz_msg, "5. Hacks are not allowed.\r\n");
        strcat( sz_msg, "6. Respect all players and Administrators.\r\n");
        ShowPlayerDialog(playerid, 1337, DIALOG_STYLE_MSGBOX, "Server Rules", sz_msg, "Close","");
        return 1;
}

COMMAND:f(playerid, params[])
{
        if(IsPlayerInAnyVehicle(playerid))
        {
        new currentveh;
        new Float:angle;
        currentveh = GetPlayerVehicleID(playerid);
        GetVehicleZAngle(currentveh, angle);
        SetVehicleZAngle(currentveh, angle);
        return 1;
        }
        else
        {
        return 1;
        }
}

CMD:pm(playerid, params[])
{
	new str[256], str2[256], id, Name1[MAX_PLAYER_NAME], Name2[MAX_PLAYER_NAME];
	if(sscanf(params, "us", id, str2))
	{
	    SendClientMessage(playerid, 0xFF0000FF, "Usage: {FFFFFF}/pm [id] [message]");
	    return 1;
	}
	if(!IsPlayerConnected(id)) return SendClientMessage(playerid, 0xFF0000FF, "ERROR: The player you are trying to pm is not online!");
	if(playerid == id) return SendClientMessage(playerid, 0xFF0000FF, "ERROR: You cannot private message yourself!");
	{
		GetPlayerName(playerid, Name1, sizeof(Name1));
		GetPlayerName(id, Name2, sizeof(Name2));
		format(str, sizeof(str), "Private Message has been sent to %s(ID %d): %s", Name2, id, str2);
		SendClientMessage(playerid, 0xFF0000FF, str);
		format(str, sizeof(str), "Private Message has been sent from %s(ID %d): %s", Name1, playerid, str2);
		SendClientMessage(id, 0xFF0000FF, str);
	}
	return 1;
}

CMD:kill(playerid, params[])
{
    SetPlayerHealth(playerid, 0);
    return 1;
}

CMD:afk(playerid, params[])
{
	new name[MAX_PLAYER_NAME], string[128];
	GetPlayerName(playerid, name, sizeof(name));
	if( afk{ playerid } ) SendClientMessage(playerid, 0xFF9900AA, "{FFFFFF}You're already {FF0000}AFK!");
	else
	{
	afk{ playerid } = true;
	SetPlayerHealth(playerid, 99999);
	SetPlayerArmour(playerid, 99999);
	TogglePlayerControllable(playerid, 0);
	format(string, sizeof(string), "{FF0000}%s {FFFFFF}is now {FF0000}AFK!", name);
	SendClientMessageToAll(0xFF9900AA, string);
	}
	return 1;
}

CMD:back(playerid, params[]) {
	if( !afk{ playerid } ) SendClientMessage(playerid, 0xFF9900AA, "{FFFFFF}You aren't {FF0000}AFK!");
	else
	{
	new name[MAX_PLAYER_NAME], string[128];
	GetPlayerName(playerid, name, sizeof(name));
	SetPlayerHealth(playerid, 99);
	SetPlayerArmour(playerid, 99);
	TogglePlayerControllable(playerid, 1);
	format(string, sizeof(string), "{FF0000}%s {FFFFFF}is now {FF0000}BACK!", name);
	SendClientMessageToAll(0x33AA33AA, string);
	afk{ playerid } = false;
	}
	return 1;
}

CMD:cmenu(playerid, params[])
{
    ShowPlayerDialog(playerid, 1, DIALOG_STYLE_LIST, "Player Color Changer", "{00FF00}Green\n{FF0000}Red\n{FFFF00}Yellow\n{0000FF}Blue\n{800080}Purple\n{FFFFFF}White\n{000000}Black\n{EDA532}Orange\n{FFD000}Gold", "Ok", "Cancel");
    return 1;
}
    
CMD:derby1(playerid, params[])
{
    new pName[MAX_PLAYER_NAME];
    new string[128];
    GetPlayerName(playerid, pName, sizeof(pName));
   	SetPlayerPos(playerid, 1539.868164, -1135.097900, 135.828125 );
