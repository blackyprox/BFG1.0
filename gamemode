    GivePlayerWeapon(playerid, 46, 1);
    new pName[MAX_PLAYER_NAME];
    new string[128];
    GetPlayerName(playerid, pName, sizeof(pName));
   	SetPlayerPos(playerid, 2463.917968, 1518.445068, 540.967529 );
   	SetPlayerFacingAngle(playerid, 276.8851);
   	SetCameraBehindPlayer(playerid);
    format(string, sizeof(string), "%s [ID:%d] has gone to Base Jump 32 (/bj32)", pName, playerid);
    SendClientMessageToAll(0xFFFF00AA, string);
    GameTextForPlayer(playerid, "~w~Welcome To Base Jump 32! ~G~", 5000, 5);
    return 1;
}

CMD:smalldm(playerid, params[])
{
    GivePlayerWeapon(playerid, 24, 250);
    GivePlayerWeapon(playerid, 25, 150);
	GivePlayerWeapon(playerid, 29, 300);
	GivePlayerWeapon(playerid, 30, 275);
    new pName[MAX_PLAYER_NAME];
    new string[128];
    GetPlayerName(playerid, pName, sizeof(pName));
   	SetPlayerPos(playerid, 1357.1898,-1565.1184,85.5469 );
   	SetPlayerFacingAngle(playerid, 276.8851);
   	SetCameraBehindPlayer(playerid);
    format(string, sizeof(string), "%s [ID:%d] has gone to Small Deathmatch (/smalldm)", pName, playerid);
    SendClientMessageToAll(0xFFFF00AA, string);
    GameTextForPlayer(playerid, "~w~Welcome To Small Deathmatch! ~G~", 5000, 5);
    return 1;
}

CMD:bigrally(playerid, params[])
{
	if(IsPlayerInAnyVehicle(playerid))
 	{
  		if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
        {
        	new pName[MAX_PLAYER_NAME];
			new string[128];
			GetPlayerName(playerid, pName, sizeof(pName));
 		    SetVehiclePos(GetPlayerVehicleID(playerid), 833.5510,-1977.9570,191.9546 );
         	SetVehicleZAngle(GetPlayerVehicleID(playerid), 264.7289);
          	SetCameraBehindPlayer(playerid);
			format(string, sizeof(string), "%s [ID:%d] has gone to Big Rally (/bigrally)", pName, playerid);
			SendClientMessageToAll(0xFFFF00AA, string);
			GameTextForPlayer(playerid, "~w~Welcome To Big Rally! ~G~", 5000, 5);
    	}
     	else
      	{
       		SendClientMessage(playerid, 0xCC0000AA, "[ERROR]: You need to be the driver!");
       	}
   	}
    else
    {
   		new pName[MAX_PLAYER_NAME];
		new string[128];
		GetPlayerName(playerid, pName, sizeof(pName));
    	SetPlayerPos(playerid, 833.5510,-1977.9570,191.9546 );
    	SetPlayerFacingAngle(playerid, 276.8851);
    	SetCameraBehindPlayer(playerid);
		format(string, sizeof(string), "%s [ID:%d] has gone to Big Rally (/bigrally)", pName, playerid);
		SendClientMessageToAll(0xFFFF00AA, string);
		GameTextForPlayer(playerid, "~w~Welcome To Big Rally! ~G~", 5000, 5);
 	}
    return 1;
}

